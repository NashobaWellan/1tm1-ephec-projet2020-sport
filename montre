ALTER PROCEDURE "DBA"."getMontreSportif"()
result (id char(255),sportif char(50), sportifPrenom char(30)/*, sportt char(30),*//*, tournoiId char(255)*/)
BEGIN
 	select joueurId, joueurNom, joueurPrenom/*, sport*///, tournoiId
 	from tbjoueur
    ORDER BY joueurNom    
/*, tbsport,*/
    //JOIN tbparticipation on tbjoueur.joueurId = tbparticipation.joueurId
    //GROUP BY joueurNom || joueurPrenom || tournoiId
END

CREATE SERVICE "joueur" TYPE 'JSON' AUTHORIZATION OFF USER "DBA" METHODS 'GET' AS call "dba"."getMontreSportif"();
